<?xml version="1.0" encoding = "iso-8859-1"?>

	
  <STYLE name="title" halignment="center" fontsize="14"/>
  <STYLE name="empresa" halignment="center" fontsize="18" fontweight="bold"/>
  <STYLE name="header" fontsize="11" fontweight="bold" 
    halignment="left"
    textcolor="black" backcolor="#F0F0F0" bordercolor="#F0F0F0" />
	
	
  <STYLE name="pagenumber" halignment="left"
    fontsize="8" textcolor="black"/>

  <STYLE name="detail" 
    fontsize="10" fontweight="normal" 
    textcolor="black" backcolor="white" adjustment="grow" />
	
  <STYLE name="footer" fontsize="8" fontweight="normal" textcolor="black"/>

  <STYLE name="number" halignment="right"/>
  <STYLE name="total" textcolor="blue" fontweight="bold"/>
  <STYLE name="date" format="%d/%m/%y"/>
  <STYLE name="euros"  mask="###,###,##0.00 e" halignment="left"/>
  <STYLE name="pounds"  mask="###,###,##0.00 £" halignment="left"/>
  <STYLE name="currency" fontfamily="courier" mask="###,###,##0.00" halignment="right"/>
  <STYLE name="currency-total" fontfamily="courier" mask="###,###,##0.00" halignment="right"
    textcolor="blue" fontweight="bold"/>
  