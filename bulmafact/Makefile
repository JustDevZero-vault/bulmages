#############################################################################
# Makefile for building: bulmafact
# Generated by qmake (1.06c) (Qt 3.2.3) on: Sat Jul 24 22:39:27 2004
# Project:  bulmafact.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile bulmafact.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
SUBTARGETS =	 \
		sub-lib \
		sub-src

first: all

all: Makefile $(SUBTARGETS)

lib/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "lib" || $(MKDIR) "lib"
	cd lib && $(QMAKE) lib.pro -o $(MAKEFILE)
sub-lib: lib/$(MAKEFILE) FORCE
	cd lib && $(MAKE) -f $(MAKEFILE)

src/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src" || $(MKDIR) "src"
	cd src && $(QMAKE) src.pro -o $(MAKEFILE)
sub-src: src/$(MAKEFILE) FORCE
	cd src && $(MAKE) -f $(MAKEFILE)

Makefile: bulmafact.pro  /usr/lib/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile bulmafact.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile bulmafact.pro

all: $(SUBTARGETS)
qmake_all: lib/$(MAKEFILE) src/$(MAKEFILE)
	 ( [ -d lib ] && cd lib ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d src ] && cd src ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d lib ] && cd lib ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install: all  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

